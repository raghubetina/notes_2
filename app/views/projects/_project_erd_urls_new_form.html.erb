<!-- Form erd_url start -->
<%= form_with(model: @erd_url, local: true) do |f| %>
<!-- Form erd_url end -->

  <!-- Label and input for project_id start -->
  <!-- Input for project_id start -->
  <%= f.hidden_field :project_id, value: @project.id %>
  <!-- Input for project_id end -->
  <!-- Label and input for project_id end -->

  <!-- Label and input for ideas_url start -->
  <div class="form-group">
    <%= f.label :ideas_url %>

    <!-- Input for ideas_url start -->
    <%= f.text_field :ideas_url, class: "form-control" %>
    <!-- Input for ideas_url end -->
  </div>
  <!-- Label and input for ideas_url end -->

  <!-- Label and input for image start -->
  <div class="form-group">
    <%= f.label :image %>

    <!-- Input for image start -->
    <%= f.text_field :image, class: "form-control" %>
    <!-- Input for image end -->
  </div>
  <!-- Label and input for image end -->

  <%= f.submit "Create erd url", class: "btn btn-block btn-outline-secondary" %>
<% end %>
